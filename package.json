{
	"author": "Grant Lonie",
	"name": "canari-swipe",
	"version": "1.2.2",
	"description": "Swiping library designed for Canari platform",
	"homepage": "https://github.com/GeeDollaHolla/canari-swipe#readme",
	"license": "MIT",
	"main": "index.js",
	"files": [
		"index.js"
	],
	"scripts": {
		"start": "parcel ./src/index.html",
		"test": "jest --watch",
		"build": "babel ./src/Swiper.js --out-file index.js --presets=es2015,react",
		"demo-build": "parcel build ./src/index.js --out-dir docs"
	},
	"babel": {
		"presets": [
			"react",
			"env"
		]
	},
	"jest": {
		"testURL": "http://localhost",
		"setupFiles": [
			"<rootDir>/src/tests/setupTests.js"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
			"\\.(css|less)$": "<rootDir>/src/tests/__mocks__/styleMock.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GeeDollaHolla/canari-swipe.git"
	},
	"keywords": [
		"react",
		"swiper",
		"swipe",
		"canari"
	],
	"bugs": {
		"url": "https://github.com/theAndrewCline/parcel-trial/issues"
	},
	"peerDependencies": {
		"react": "^16.4.1",
		"react-dom": "^16.4.1"
	},
	"devDependencies": {
		"react-test-renderer": "^16.4.1",
		"babel-jest": "^23.2.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"@babel/cli": "^7.0.0-rc.1",
		"@babel/core": "^7.0.0-rc.1",
		"babel-cli": "^6.26.0",
		"enzyme": "^3.4.1",
		"enzyme-adapter-react-16": "^1.2.0",
		"enzyme-to-json": "^3.3.4",
		"jest": "^23.3.0",
		"parcel-bundler": "^1.9.4"
	}
}
