{
	"author": "Grant Lonie",
	"name": "canari-swipe",
	"version": "2.1.12",
	"description": "Swiping library designed for Canari platform",
	"homepage": "https://github.com/GeeDollaHolla/canari-swipe#readme",
	"license": "MIT",
	"files": [
		"index.js",
		"index.d.ts"
	],
	"scripts": {
		"start": "parcel ./src/index.html",
		"test": "jest --watch",
		"prebuild": "rm -rf docs",
		"build": "npm run build:swiper && npm run build:docs",
		"build:swiper": "babel ./src/Swiper.tsx --out-file index.js --presets=@babel/preset-env,@babel/preset-typescript,@babel/preset-react,minify && tsc",
		"build:docs": "parcel build ./src/index.html --no-source-maps --dist-dir docs"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"jest": {
		"testURL": "http://localhost",
		"setupFiles": [
			"<rootDir>/src/tests/setupTests.js"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
			"\\.(css|less)$": "<rootDir>/src/tests/__mocks__/styleMock.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GeeDollaHolla/canari-swipe.git"
	},
	"keywords": [
		"react",
		"swiper",
		"swipe",
		"canari"
	],
	"peerDependencies": {
		"react": "^16.4.1 || ^17.0.0 || ^18.0.0",
		"react-dom": "^16.4.1 || ^17.0.0 || ^18.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.21.4",
		"@types/react": "^18.0.38",
		"@types/react-dom": "^18.0.11",
		"babel-preset-minify": "^0.5.2",
		"jest": "^29.5.0",
		"parcel": "^2.8.3",
		"process": "^0.11.10",
		"typescript": "^5.0.4"
	}
}
