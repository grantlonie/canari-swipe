{
	"author": "Grant Lonie",
	"name": "canari-swipe",
	"version": "2.1.12",
	"description": "Swiping library designed for Canari platform",
	"homepage": "https://github.com/GeeDollaHolla/canari-swipe#readme",
	"license": "MIT",
	"files": [
		"index.js",
		"index.d.ts"
	],
	"scripts": {
		"start": "parcel ./src/index.html",
		"test": "jest --watch",
		"build": "parcel build ./src/index.html --no-source-maps --public-url=.",
		"build:swiper": "babel ./src/Swiper.tsx --out-file index.js --presets=@babel/preset-env,@babel/preset-typescript,@babel/preset-react,minify && tsc -p ./tsconfig.swiper.json"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GeeDollaHolla/canari-swipe.git"
	},
	"keywords": [
		"react",
		"swiper",
		"swipe",
		"canari"
	],
	"peerDependencies": {
		"react": "^16.4.1 || ^17.0.0 || ^18.0.0",
		"react-dom": "^16.4.1 || ^17.0.0 || ^18.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.21.4",
		"@emotion/babel-plugin": "^11.9.2",
		"@emotion/react": "^11.9.3",
		"@parcel/transformer-typescript-tsc": "^2.8.3",
		"@types/react": "^18.0.38",
		"@types/react-dom": "^18.0.11",
		"babel-preset-minify": "^0.5.2",
		"jest": "^29.5.0",
		"parcel": "^2.8.3",
		"process": "^0.11.10",
		"typescript": "^5.0.4"
	}
}
